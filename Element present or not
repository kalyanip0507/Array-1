#include<stdio.h>
int main(){
    int a[100],num,n,i=0, found=0;
    printf("enter size");
    scanf("%d",&n);
    printf("enter array elements");
    for(i=0;i<n;i++){'9'
        scanf("%d",&a[i]);
    }
    printf("enter the element you want to search");
    scanf("%d",&num);
    for(i=0;i<n;i++)
    {
        if(num==a[i])
            {
        found=1;
        printf("%d found at %d",num,i);
    }else{
        found=-1;
    }
    }

    return 0;
}
